{
  "name": "car",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "start": "ts-node-dev --exit-child index.ts",
    "tsn": "ts-node src/index.ts",
    "build": "docker build -t maitriyogin/car.",
    "push": "docker push maitriyogin/car",
    "db": "yarn build && yarn push",
    "ka": "kubectl apply -f ../infra/k8s/local/car-depl.yaml",
    "kr": "kubectl rollout restart deployment car-depl",
    "kd": "kubectl delete deployment car-depl",
    "dkr": "yarn db && yarn kr",
    "apply": "yarn db && yarn ka"
  },
